	/**
	 * @param args
	 */
	public void init() {
		// TODO Auto-generated method stub
		Button b1,b2,b3;
		b1 = new Button("OK");
		b2 = new Button("Cancel");
		b3 = new Button("Exit");
		
		add(b1);
		add(b2);
		add()

	}

}
