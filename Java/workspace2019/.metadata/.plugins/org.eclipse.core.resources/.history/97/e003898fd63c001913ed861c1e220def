package exp2;

import java.awt.*;
import java.applet.*;

/**
 * @author Anmoldeep
 *
 */
public class BordLayoutDemo  extends Applet{
	
	Button left, right, top, bottom;
	
	@Override
	public void init() {
		// TODO Auto-generated method stub
		left = new Button("Left");
		right = new Button("Right");
		top = new Button("Top");
		bottom = new Button("Bottom");
		
		
	}
}
